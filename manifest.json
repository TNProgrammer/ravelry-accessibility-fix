{
  "manifest_version": 2,
  "name": "Ravelry Accessibility Fix",
  "version": "0.6.0",
  "description": "Color fix for Ravelry",
  "icons": {
    "16": "icons/16x16.png",
    "24": "icons/24x24.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "content_scripts": [
    {
      "css": ["styles.css"],
      "js": ["lodash.js", "content.js"],
      "matches": ["*://*.ravelry.com/*"]
    }
  ]
}